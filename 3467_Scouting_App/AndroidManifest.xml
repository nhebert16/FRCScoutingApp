<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.robotics_test_project"
    android:screenOrientation="portrait"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/wwlogo1"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.robotics_test_project.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.robotics_test_project.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.example.myfirstapp.MainActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.myfirstapp.MainActivity" />
        </activity>
        <activity
            android:name="com.example.robotics_test_project.MatchScoutingAuto"
            android:label="@string/title_activity_match_scouting_auto"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.MatchScoutingMan"
            android:label="@string/title_activity_match_scouting_man"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.PitScouting"
            android:label="@string/title_activity_pit_scouting"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.MatchScoutingIntro"
            android:label="@string/title_activity_match_scouting_intro"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.TeamStats"
            android:label="@string/title_activity_team_stats"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.MatchReview"
            android:label="@string/title_activity_match_review"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.BluetoothPairedDeviceSelect"
            android:label="@string/title_activity_bluetooth_paired_device_select"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.UploadLink"
            android:label="@string/title_activity_upload_link" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.TextSender"
            android:label="@string/title_activity_text_sender" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.ServerSettings"
            android:label="@string/title_activity_server_settings" >
        </activity>
        <activity
            android:name="com.example.robotics_test_project.ServerLink"
            android:label="@string/title_activity_server_link" >
        </activity>
    </application>

</manifest>
